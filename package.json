{
  "name": "boilerplate-express-polymer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db": "mkdir -p ./db/dev && mongod --dbpath ./db/dev/",
    "start": "./node_modules/.bin/nodemon app.js",
    "debug": "./node_modules/.bin/node-inspector",
    "lint": "./node_modules/.bin/jshint . > ./generated/jshint.txt; exit 0",
    "check-style": "./node_modules/.bin/jscs . > ./generated/jscs.txt; exit 0",
    "test:back": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./generated/test/back --report html -- --recursive --watch -R spec -r should app/tests/",
    "test:front": "./node_modules/karma/bin/karma start",
    "test": "npm run-script test:back && npm run-script test:front",
    "docs:back": "./node_modules/.bin/jsdoc -c ./jsdoc.json ./app",
    "docs:api": "./node_modules/.bin/apidoc -i ./app -o ./generated/docs/api",
    "docs": "npm run-script docs:back && npm run-script docs:api"
  },
  "dependencies": {
    "async": "^0.9.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.10.0",
    "compression": "^1.2.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^0.4.1",
    "consolidate": "^0.10.0",
    "cookie-parser": "~1.3.2",
    "debug": "^1.0.4",
    "email-templates": "^1.1.2",
    "express": "^4.10.6",
    "express-session": "^1.9.3",
    "helmet": "^0.5.3",
    "intl": "^0.1.4",
    "lodash": "^2.4.1",
    "method-override": "^2.3.0",
    "moment": "^2.8.4",
    "mongoose": "^3.8.20",
    "morgan": "^1.5.0",
    "nodemailer": "^1.3.0",
    "nodemailer-smtp-transport": "^0.1.13",
    "passport": "^0.2.1",
    "passport-facebook": "^1.0.3",
    "passport-google": "^0.3.0",
    "passport-google-oauth": "^0.1.5",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.2",
    "prerender-node": "^1.2.0",
    "serve-favicon": "^2.1.6",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "apidoc": "^0.12.0",
    "brfs": "^1.2.0",
    "browserify": "^6.3.4",
    "browserify-shim": "^3.8.1",
    "debowerify": "^0.9.1",
    "event-stream": "^3.1.7",
    "gulp": "^3.8.10",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.4.2",
    "gulp-concat-css": "^1.1.1",
    "gulp-cssmin": "^0.1.6",
    "gulp-ignore": "^1.2.1",
    "gulp-ng-annotate": "^0.4.1",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^1.2.4",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "istanbul": "^0.3.5",
    "jscs": "^1.8.1",
    "jsdoc": "^3.3.0-alpha11",
    "jshint": "^2.5.11",
    "karma": "^0.12.28",
    "karma-browserify": "^1.0.1",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.3",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "minimist": "^1.1.0",
    "mocha": "^2.0.1",
    "nodemon": "^1.2.1",
    "should": "^4.4.1",
    "supertest": "^0.15.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "brfs",
      "debowerify"
    ]
  }
}
